{
  "name": "@tinijs/cli",
  "version": "0.17.0",
  "description": "The CLI for the TiniJS framework and beyond.",
  "author": "Nhan Lam",
  "homepage": "https://tinijs.dev",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": "./dist/public-api.js"
  },
  "bin": {
    "tini": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "!**/*.spec.*"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc && chmod +x ./dist/cli/index.js",
    "deploy": "npm run build && npm publish",
    "test": "vitest run",
    "lint": "gts lint",
    "fix": "gts fix"
  },
  "keywords": [
    "TiniJS",
    "CLI"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tinijs/tinijs.git"
  },
  "bugs": {
    "url": "https://github.com/tinijs/tinijs/issues"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "@tinijs/project": "^0.17.0",
    "adm-zip": "^0.5.10",
    "axios": "^1.6.7",
    "change-case": "^5.4.3",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.6.0",
    "citty": "^0.1.6",
    "colorette": "^2.0.20",
    "concurrently": "^8.2.2",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "jiti": "^1.21.0",
    "knitwork": "^1.1.0",
    "open": "^10.0.4",
    "ora": "^8.0.1",
    "pathe": "^1.1.2",
    "picomatch": "^4.0.2",
    "prettier": "^3.2.5",
    "recursive-readdir": "^2.2.3",
    "superstatic": "^9.0.3",
    "typescript": "~5.3.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/picomatch": "^2.3.3",
    "@types/recursive-readdir": "^2.2.4"
  }
}
