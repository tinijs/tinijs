{
  "name": "@tinijs/ui",
  "version": "0.20.0",
  "description": "A next generation web UI system.",
  "author": "Nhan Lam",
  "homepage": "https://tinijs.dev",
  "license": "MIT",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": "./dist/public-api.js",
    "./dist/*": "./dist/*",
    "./cli": "./dist/cli/public-api.js",
    "./cli-expansion": "./dist/cli/expansion.js"
  },
  "files": [
    "dist",
    "!**/*.spec.*"
  ],
  "scripts": {
    "tini": "node ../cli/dist/cli/index.js",
    "build": "rimraf ./dist && tsc && npm run tini -- ui build",
    "lint": "gts lint",
    "fix": "gts fix"
  },
  "keywords": [
    "TiniJS",
    "Web Components",
    "UI Library"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinijs/tinijs.git"
  },
  "bugs": {
    "url": "https://github.com/tinijs/tinijs/issues"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "@tinijs/cli": "^0.20.0",
    "@tinijs/core": "^0.20.0",
    "@tinijs/project": "^0.20.0",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "destr": "^2.0.3",
    "esbuild": "^0.20.2",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "isomorphic-dompurify": "^2.9.0",
    "knitwork": "^1.1.0",
    "lit": "^3.1.2",
    "pathe": "^1.1.2",
    "svgo": "^3.2.0",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@lit/react": "^1.0.2",
    "@types/fs-extra": "^11.0.4",
    "react": "^18.2.0"
  }
}
